/**index.wxss**/
.container{
  height: 100%;
  margin: 0;
  padding: 0;
}
.list{
  background-color: #fff;
  position: relative;
}
.image{
  width: 400px;
  height: 200px;
}
.image image {
  display: flex;
  width: 100%;
  height: 100%;
  display: block;
}
.text text{
  font-size: 14px;
  font-weight: bold;
  color: #fff;
  position: absolute;
  bottom: 20px;
  left: 50px;
}
.text image{

  position: absolute;
  width: 18px;
  height: 35px;
  bottom: 10px;
  left: 50px;
  color: #fff;
}

